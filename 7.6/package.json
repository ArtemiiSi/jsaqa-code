		"test:ChromeMobileheaded": "npx cypress run --browser chrome --headed --config-file cypress.config.mobile.js",
    "test:ChromeMobileHeadless": "npx cypress run --browser chrome --headless --config-file cypress.config.mobile.js",
    "test:ChromiumHeaded": "npx cypress run --browser chromium --headed --config-file cypress.config.js",
		"test:ChromiumHeadless": "npx cypress run --browser chromium --headless --config-file cypress.config.js",
    "test:ElectronMobileHeaded": "npx cypress run --browser electron  --headed --config-file cypress.config.mobile.js",
		"test:ElectronMobileHeadless": "npx cypress run --browser electron  --headless --config-file cypress.config.mobile.js"
  },
